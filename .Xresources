!
!! URxvt Appearance
!! URxvt.transparent: true
!! URxvt.shading: 12
URxvt.depth: 32
URxvt.background: [88]#191919
!! URxvt.font:xft:Dejavu Sans Mono:size=9:antialias=True, \
!! xft:HYZhongYuanJ:size=13:antialias=True
URxvt.font: xft:Consolas NF:size=13:antialias=True, \
              xft:WenQuanYi Micro Hei:size=12:antialias=True,\
              xft:JoyPixels:size=13:antialias=True
!URxvt.Font: xft:Unifont:size=13
!! urxvt.boldItalicFont: xft:Droid Sans Mono Nerd Font Complete:style=Bold Italic:size=8
!! URxvt.letterSpace: -1
!! URxvt.lineSpace: 0
!! URxvt.geometry: 92x24
!! URxvt.internalBorder: 24
URxvt.cursorBlink: true
!! URxvt.cursorUnderline: false
!! URxvt.saveline: 2048
URxvt.scrollBar: false
!! URxvt.scrollBar_right: false
!! URxvt.urgentOnBell: true
URxvt.iso14755: false
URxvt.iso14755_52: false



!! Common Keybinds for Navigations
!! scroll up
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.M-k: command:\033]720;1\007
!! scroll down
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.M-j: command:\033]721;1\007
! URxvt.keysym.Control-Up: \033[1;5A
! URxvt.keysym.Control-Down: \033[1;5B
! URxvt.keysym.Control-Right: \033[1;5C
! URxvt.keysym.Control-Left: \033[1;5D

!! Copy Paste & Other Extensions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.M-V: eval:paste_clipboard
URxvt.keysym.M-C: eval:selection_to_clipboard
!! URxvt.perl-lib: .config/urxvt/urxvt-perls/
URxvt.perl-ext-common: default,url-select,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.urlLauncher: qutebrowser
URxvt.underlineURLs: true
URxvt.urlButton: 1

!! https://addy-dclxvi.github.io/post/configuring-urxvt/
